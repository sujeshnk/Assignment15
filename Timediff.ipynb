        "birth = '28-06-1983'\n",
        "import datetime\n",
        "now = datetime.datetime.now()\n",
        "now\n",
        "birth\n",
        "then = datetime.datetime.strptime(birth,'%d-%m-%Y')\n",
        "then\n",
        "elapsed = now - then\n",
        "elapsed\n",
        "elapsed.days/7\n",
        "elapsed.days/30\n",
        "elapsed.days/365"
